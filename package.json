{
  "name": "modulink",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node --transpile-only src/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --delay 1 --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "jest:watch": "jest --setupFiles dotenv/config --watch",
    "jest:test": "jest --setupFiles dotenv/config --forceExit --detectOpenHandles",
    "lint": "eslint src/**/*.ts",
    "lint:test": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",

    "@types/md5": "^2.2.1",
    "@types/multer": "^1.4.4",
    "@types/multer-s3": "^2.7.8",
    "@types/mysql": "^2.15.15",
    "@types/mysql2": "github:types/mysql2",
    "@types/nodemon": "^1.19.0",
    "@types/pg": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "aws-sdk": "^2.783.0",
    "babel-jest": "^26.6.3",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "envalid": "^6.0.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "helmet": "^4.1.0",
    "hpp": "^0.2.3",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
